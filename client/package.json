{
  "name": "sync-form-project-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "rm dist/* && npm run build:min:js & npm run build:min:css",
    "build": "npm run build:js & npm run build:css",
    "build:js": "browserify -d index.js -o dist/app.js",
    "build:min:js": "browserify index.js | uglifyjs -o dist/app.js",
    "build:css": "stylus -m index.styl -o dist/app.css",
    "build:min:css": "stylus < index.styl | cleancss -o dist/app.css",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:js": "watchify -d index.js -o dist/app.js",
    "watch:css": "stylus -w -m index.styl -o dist/app.css"
  },
  "browserify": {
    "extensions": [
      ".js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "react": "^0.14.3",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.0",
    "socket.io-client": "^1.3.7",
    "stylus": "^0.53.0",
    "vdux": "^1.0.1",
    "virtex": "^0.1.17",
    "virtex-dom": "^0.1.20",
    "watchify": "^3.6.1"
  }
}
