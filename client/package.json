{
  "name": "sync-form-project-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "rm dist/* && NODE_ENV=production npm run build:tmpl & npm run build:min:js & npm run build:min:css",
    "build": "npm run build:js & npm run build:css & npm run build:tmpl",
    "build:js": "browserify -d index.js -o dist/app.js",
    "build:min:js": "browserify index.js | uglifyjs -o dist/app.js",
    "build:css": "stylus -m index.styl -o dist/app.css",
    "build:min:css": "stylus < index.styl | cleancss -o dist/app.css",
    "build:tmpl": "jade -P index.jade --out dist",
    "watch": "npm run watch:js & npm run watch:css & npm run watch:tmpl",
    "watch:js": "watchify -d index.js -o dist/app.js",
    "watch:css": "stylus -w -m index.styl -o dist/app.css",
    "watch:tmpl": "jade -w index.jade --out dist"
  },
  "browserify": {
    "extensions": [
      ".js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "browserify": "^12.0.1",
    "socket.io-client": "^1.3.7"
  }
}
